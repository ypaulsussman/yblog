{
  "name": "yblog",
  "description": "A minimal, lightweight and mobile-first blog adapted from the gatsby-starter-lumen starter",
  "main": "n/a",
  "scripts": {
    "develop": "yarn run clean && GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "yarn run clean && gatsby build",
    "clean": "rimraf .cache public",
    "lint:js": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:scss\"",
    "test": "jest --config ./tests/jest-config.js",
    "test:coverage": "jest --coverage --config ./tests/jest-config.js",
    "test:watch": "jest --watch --config ./tests/jest-config.js",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build"
  },
  "repository": "https://github.com/ypaulsussman/yblog",
  "author": "Like 90% Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react",
    "graphql",
    "prismjs",
    "netlify",
    "blog",
    "static",
    "minimal",
    "pagination",
    "categories",
    "tags"
  ],
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "bluebird": "^3.5.5",
    "classnames": "^2.2.6",
    "codecov": "^3.5.0",
    "gatsby": "^2.8.3",
    "gatsby-cli": "^2.7.53",
    "gatsby-link": "^2.1.1",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-feed": "^2.2.2",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.1.3",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.0.14",
    "gatsby-remark-katex": "^3.0.4",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.21",
    "invariant": "^2.2.4",
    "katex": "^0.11.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "browserslist": "4.7.0",
    "concurrently": "4.1.2",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-watch": "6.0.1",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "lost": "8.3.1",
    "postcss-pxtorem": "4.0.1",
    "react-test-renderer": "16.10.1",
    "rimraf": "3.0.0",
    "stylelint": "11.0.0",
    "stylelint-config-recommended-scss": "4.0.0",
    "stylelint-scss": "3.11.1"
  }
}
