{
  "name": "yblog",
  "description": "A minimal, lightweight and mobile-first blog adapted from the gatsby-starter-lumen starter",
  "main": "n/a",
  "scripts": {
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build",
    "build": "yarn run clean && gatsby build",
    "clean": "rimraf .cache public",
    "develop": "yarn run clean && GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "lint:js": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:scss\"",
    "serve": "gatsby serve",
    "test": "jest --config ./tests/jest-config.js",
    "test:coverage": "jest --coverage --config ./tests/jest-config.js",
    "test:watch": "jest --watch --config ./tests/jest-config.js"
  },
  "repository": "https://github.com/ypaulsussman/yblog",
  "author": "Like 90% Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react",
    "graphql",
    "prismjs",
    "netlify",
    "blog",
    "static",
    "minimal",
    "pagination",
    "categories",
    "tags"
  ],
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "codecov": "^3.6.5",
    "gatsby": "^2.19.17",
    "gatsby-cli": "^2.8.29",
    "gatsby-link": "^2.2.29",
    "gatsby-plugin-catch-links": "^2.1.25",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-katex": "^3.1.24",
    "gatsby-remark-prismjs": "^3.3.31",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.32",
    "gatsby-remark-smartypants": "^2.1.21",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.51",
    "gatsby-transformer-sharp": "^2.3.14",
    "invariant": "^2.2.4",
    "katex": "^0.11.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "autoprefixer": "9.7.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "browserslist": "4.8.7",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "eslint-watch": "6.0.1",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-cli": "25.1.0",
    "lost": "8.3.1",
    "postcss-pxtorem": "4.0.1",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.2",
    "stylelint": "13.2.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.14.2"
  }
}
