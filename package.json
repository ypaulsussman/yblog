{
  "name": "yblog",
  "description": "A minimal, lightweight and mobile-first blog adapted from the gatsby-starter-lumen starter",
  "main": "n/a",
  "scripts": {
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build",
    "build": "yarn run clean && gatsby build",
    "clean": "rimraf .cache public",
    "develop": "yarn run clean && GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "lint:js": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:scss\"",
    "serve": "gatsby serve",
    "test": "jest --config ./tests/jest-config.js",
    "test:coverage": "jest --coverage --config ./tests/jest-config.js",
    "test:watch": "jest --watch --config ./tests/jest-config.js"
  },
  "repository": "https://github.com/ypaulsussman/yblog",
  "author": "Like 90% Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react",
    "graphql",
    "prismjs",
    "netlify",
    "blog",
    "static",
    "minimal",
    "pagination",
    "categories",
    "tags"
  ],
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "gatsby": "^2.24.91",
    "gatsby-cli": "^2.12.113",
    "gatsby-link": "^2.4.16",
    "gatsby-plugin-catch-links": "^2.3.5",
    "gatsby-plugin-feed": "^2.5.20",
    "gatsby-plugin-manifest": "^2.4.37",
    "gatsby-plugin-netlify": "^2.3.25",
    "gatsby-plugin-offline": "^3.2.38",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sass": "^2.3.22",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-remark-autolink-headers": "^2.3.16",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-images": "^3.3.40",
    "gatsby-remark-katex": "^3.3.14",
    "gatsby-remark-prismjs": "^3.5.16",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^2.4.17",
    "gatsby-remark-smartypants": "^2.3.13",
    "gatsby-source-filesystem": "^2.3.37",
    "gatsby-transformer-remark": "^2.8.47",
    "gatsby-transformer-sharp": "^2.5.20",
    "invariant": "^2.2.4",
    "katex": "^0.12.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "autoprefixer": "10.0.1",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.1",
    "browserslist": "4.14.5",
    "concurrently": "5.3.0",
    "eslint": "7.12.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-watch": "7.0.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.1",
    "jest-cli": "26.6.1",
    "lost": "8.3.1",
    "postcss-pxtorem": "5.1.1",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "stylelint": "13.7.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0"
  }
}
